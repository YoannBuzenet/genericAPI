    "@bugsnag/js": "^7.9.2",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "fastify": "^3.14.0",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.5.0",
    "sequelize": "^6.5.1",
    "sequelize-cli": "^6.2.0",
    "stripe": "^8.138.0"
  }
